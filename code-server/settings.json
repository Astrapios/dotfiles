{
    "workbench.colorCustomizations": {
        "terminalCursor.foreground": "#00ffaa"
    },
    "editor.fontSize": 18,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.lineNumbers": "relative",
    "debug.inlineValues": "on",
    "update.mode": "manual",
    "workbench.colorTheme": "Monokai",
    "files.autoSave": "afterDelay",
    "files.restoreUndoStack": true,
    "git.autofetch": true,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.macOptionIsMeta": true,
    "terminal.integrated.profiles.windows": {
        "PowerShell": null,
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [
                "/K",
                "C:/Users/ryujis/miniconda3/Scripts/activate.bat C:/Users/ryujis/miniconda3 & conda activate research"
            ],
            "icon": "terminal-cmd"
        },
        "MSYS2": {
            "path": [
                "/msys64/usr/bin/bash.exe"
            ],
            "args": [
                "--login"
            ]
        },
        "Git Bash": null
    },
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "vim.overrideCopy": false,
    "vim.foldfix": true,
    "vim.scroll": 15,
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.leader": ",",
    "vim.easymotion": true,
    "vim.camelCaseMotion.enable": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        {
            "before": [
                "<C-d>"
            ],
            "after": [
                "1",
                "2",
                "g",
                "j"
            ]
        },
        {
            "before": [
                "<C-u>"
            ],
            "after": [
                "1",
                "2",
                "g",
                "k"
            ]
        },
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "editor.toggleFold"
            ]
        },
        {
            "before": [
                "<leader>",
                "<space>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                "workbench.action.splitEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "t"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "<leader>",
                "s"
            ],
            "after": [
                "<leader>",
                "<leader>",
                "2",
                "s"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "workbench.action.findInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "o",
                "s"
            ],
            "commands": [
                "workbench.action.showAllSymbols"
            ]
        },
        {
            "before": [
                "<leader>",
                "o",
                "a"
            ],
            "commands": [
                "workbench.action.showEditorsInActiveGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "o",
                "p"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "o"
            ],
            "commands": [
                "workbench.action.closeOtherEditors"
            ]
        },
        {
            "before": [
                "<leader>",
                "m"
            ],
            "commands": [
                "editor.action.toggleMinimap"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "commands": [
                {
                    "command": "workbench.action.openPreviousEditorFromHistory"
                },
                {
                    "command": "workbench.action.acceptSelectedQuickOpenItem"
                }
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "c"
            ],
            "commands": [
                "editor.action.commentLine",
                "extension.vim_escape"
            ]
        },
        {
            "before": [
                "p"
            ],
            "after": [
                "p",
                "g",
                "v",
                "y"
            ]
        }
    ],
    "[python]": {
        "explicitFolding.rules": [
            {
                "begin": "{{{",
                "end": "}}}",
                "autoFold": false
            },
            {
                "indentation": true,
                "offSide": true,
                "autoFold": false
            }
        ],
        "explicitFolding.autoFold": "none",
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "python.condaPath": "/Users/jryu/miniforge3/condabin/conda",
    "jupyter.askForKernelRestart": false,
    "jupyter.useDefaultConfigForJupyter": false,
    "workbench.activityBar.orientation": "vertical",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.stl": "3dviewer.viewer"
    },
    "autoDocstring.startOnNewLine": true,
    "autoDocstring.docstringFormat": "numpy",
    "git.openRepositoryInParentFolders": "always",
    "flake8.args": [
        "--ignore=E203,E501,W503,F401"
    ],
    "jupyter.interactiveWindow.textEditor.executeSelection": false,
    "explorer.confirmDelete": false,
    "vim.enableNeovim": true,
    "explorer.confirmDragAndDrop": false,
    "remote.SSH.remotePlatform": {
        "ec2": "linux",
        "ec2_gov": "linux"
    },
    "editor.accessibilitySupport": "off",
    "python.defaultInterpreterPath": "/Users/jisu/miniforge3/envs/array/bin/python",
    "pythonREPL.pythonRunCommand": "PYTHONPATH=./ ipython",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "explicit"
    },
    "json.schemaDownload.enable": true,
    "windsurfPyright.disableLanguageServices": true,
    "basedpyright.analysis.typeCheckingMode": "standard",
    "basedpyright.analysis.diagnosticSeverityOverrides": {
        // These are the "Based" specific flags that usually feel like "too much"
        "reportAny": "none",
        "reportUnusedCallResult": "none",
        "reportUnknownMemberType": "none",
        "reportUnknownVariableType": "none",
        "reportUnknownArgumentType": "none",
        "reportMissingTypeStubs": "none",
        "reportAttributeAccessIssue": "none",
        "reportOptionalMemberAccess": "none",
        "reportOptionalOperand": "none",
        "reportArgumentType": "none",
        "reportCallIssue": "none"
    },
    "window.newWindowProfile": "Default",
    "remote.autoForwardPortsSource": "hybrid"
}